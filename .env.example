# Kling AI API Configuration
# Copy this file to .env and fill in your values

# Required: Your Kling AI Access Key and Secret Key for JWT authentication
ACCESSKEY_API=your_access_key_here
ACCESSKEY_SECRET=your_secret_key_here

# Optional: API base URL (default shown)
KLING_API_BASE_URL=https://api.klingai.com

# Optional: JWT token expiration time in seconds (default: 1800 = 30 minutes)
KLING_TOKEN_EXPIRATION=1800

# Optional: API request timeout in seconds (default: 60)
KLING_API_TIMEOUT=60

# Optional: Maximum number of retries for failed requests (default: 3)
KLING_API_MAX_RETRIES=3